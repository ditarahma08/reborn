{
  "name": "fslightbox-react",
  "version": "1.5.1",
  "description": "Pro version of React Fullscreen Lightbox.",
  "author": "Bantha Apps Piotr Zdziarski",
  "homepage": "https://fslightbox.com/",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "production": "webpack --mode production --config ./webpack.prod.config.js --display-modules && gulp",
    "watch": "webpack-dev-server  --mode development --host 0.0.0.0",
    "cypress": "concurrently --kill-others \"webpack-dev-server --config ./webpack.cypress.config.js --mode development\" \"node ./node_modules/.bin/cypress open\""
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/demo/",
      "/dist/",
      "/tests/cypress/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|jpg|jpeg)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "collectCoverage": false
  },
  "peerDependencies": {
    "prop-types": ">=15.6.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
