    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.0.1",
    "css-loader": "^2.1.0",
    "cypress": "^7.4.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.12.0",
    "path-that-svg": "^1.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
